package quiz04;

public class Book {


/*
 * field
 *
	int width; // 너비
	int height; // 높이
	
	// method
	void set(int w, int h) {
		if( w > 0) {
			width = w;
		}
		if ( h > 0 ) {
			height = h;
		}		
	}
	void set(int n) {
		// 다른 set() 메소드를 호출해서 처리합니다.
		set(n ,n);
	}
	int getArea() {
		return  width * height;
	}*/

	String title;
	String writer;
	int price;
	void set(String t, String w, int m) {
		title = t;
		writer = w;
		price = p;
		
	void set(String t, String w) {
		set(t, w, 0);
	}
	void set(String t, int p) {
		set(t, "미상" , p);
	}
	void info() {
		String str = "제목 : " + title + ", 저자" + writer + ", 가격 : " + price + "원"
		System.out.println(str);
	}
		
	
	}
	
	
	
	
}