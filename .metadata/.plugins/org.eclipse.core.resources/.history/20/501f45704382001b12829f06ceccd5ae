package ex04_this;
/*
 * this
 * 1. this는 객체 자신을 의미합니다.
 * 2. 어떤 클래스 내부에서만 사용합니다.
 * 3. 활용
 * 		1) this.필드 (가장 주된 활용)
 * 		2) this()
 * */
public class Car {

	// field
	String model;
	String manufaturer;
	int price;
	
	// method
	void set(String model, String manufacturer, int price) {
		this.model = model;
		this.manufaturer = manufacturer;
		this.price = price;
	
	}
	
	void info() {
			System.out.println("모델명 : " + model);
			System.out.println("제조사 : " + manufaturer);
			System.out.println("가격은 : " + price);
		}

	}


